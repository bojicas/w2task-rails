<VirtualHost <%= public_ip %>:80>
  ServerName <%= server_name %>
  ServerAlias <%= server_alias %>
  DocumentRoot <%= public_path %>
	RailsBaseURI /
 
  <Directory "<%= public_path %>/">
  Options FollowSymLinks
  AllowOverride None
  Order allow,deny
  Allow from all
  </Directory>
</VirtualHost>
