<div id="workspace-header1" style="background-color: #303030;">
	<div id="blue-icons">
		<%= link_to 'Back', projects_path %>
	</div>
	<div id="efforttitle"><strong>New project</strong></div>
</div>

<% form_for(@project) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %>: <%= session[:user_login] %> #(<%= session[:user_id] %>)
    <%= f.hidden_field :user_id, :value => session[:user_id] %>,
    <%= f.label :business_id %>: <%= session[:business_name] %> #(<%= session[:business_id] %>)
    <%= f.hidden_field :business_id, :value => session[:business_id] %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', projects_path %>
